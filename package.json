{
  "name": "Wesley.is",
  "version": "1.0.0",
  "description": "Building Own Site with React",
  "config": {
    "buildDir": "./build",
    "devHost": "localhost",
    "devPort": 8080
  },
  "scripts": {
    "build": "NODE_ENV=production npm run webpack",
    "deploy": "s3cmd put -r build/ s3://wesley.is",
    "clean": "rimraf $npm_package_config_buildDir && mkdir $npm_package_config_buildDir",
    "env": "env",
    "prebuild": "npm run clean",
    "prestart": "npm install",
    "start": "NODE_ENV=development node dev-server ./webpack/config",
    "webpack": "webpack --colors --progress --config ./webpack/config"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Swelly/Projects/mySite.git"
  },
  "author": "Wesley Magness",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Swelly/Projects/mySite/issues"
  },
  "homepage": "https://github.com/Swelly/Projects/mySite#readme",
  "devDependencies": {
    "autoprefixer": "^7.1.3",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-rewire": "^1.1.0",
    "babel-runtime": "^6.26.0",
    "css-loader": "^0.28.7",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^0.11.2",
    "glob": "^7.1.2",
    "html-loader": "^0.5.1",
    "http-server": "^0.10.0",
    "json-loader": "^0.5.7",
    "lodash": "^4.17.4",
    "node-libs-browser": "^2.0.0",
    "node-sass": "^4.5.3",
    "opn": "^5.1.0",
    "react-hot-loader": "^1.3.1",
    "rimraf": "^2.6.1",
    "sass-loader": "^6.0.6",
    "source-map-loader": "^0.2.1",
    "style-loader": "^0.18.2",
    "svgo": "^0.7.2",
    "svgo-loader": "^1.2.1",
    "template-html-loader": "0.0.3",
    "url-loader": "^0.5.9",
    "webpack": "^3.5.6",
    "webpack-dev-server": "^2.7.1"
  },
  "dependencies": {
    "classnames": "^2.2.5",
    "react": "^15.6.1",
    "react-router": "^4.2.0"
  },
  "engines": {
    "node": ">=0.12.0"
  }
}
